{
  "name": "new-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel test:watch open:src lint:watch run-dev-server run-ca-public-website",
    "preinstall": "node  client/tools/nodeVersionCheck.js",
    "setup": "node  client/tools/setup/setupMessage.js && npm install && node client/tools/setup/setup.js",
    "start-message": "babel-node  client/tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "open:src": "babel-node  client/tools/srcServer.js",
    "open:dist": "babel-node  client/tools/distServer.js",
    "lint": "esw  client/webpack.config.*  client/src  client/tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir  client/dist",
    "remove-dist": "rimraf ./client/dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node  client/tools/build.js && npm run open:dist",
    "test": "jest",
    "test:CI": "babel-node  client/tools/testCi.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && cat ./client/coverage/lcov.info |  client/node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open  client/coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./client/tools/analyzeBundle.js",
    "run-dev-server": "nodemon -e js,json,gql ./api/bin/www",
    "run-ca-public-website": "nodemon -e js,json,jsx ca-scta.org/bin/www"
  },
  "dependencies": {
